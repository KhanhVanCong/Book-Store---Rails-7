  "private": "true",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.2.0",
    "@hotwired/stimulus": "^3.1.0",
    "@hotwired/turbo-rails": "^7.1.3",
    "@rails/activestorage": "^7.0.4",
    "autoprefixer": "^10.4.12",
    "bootstrap": "4.3.1",
    "esbuild": "^0.15.7",
    "jquery": "3.4.1",
